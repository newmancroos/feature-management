@model List<Employee>
@{
    ViewData["Title"] = "GetEmployee";
}
    <feature name="@nameof(FeatureFlage.ListEmployees)">
        <h1>GetEmployee</h1>
        <ul>
            @foreach (var employee in Model)
            {
                <li>@employee.FirstName @employee.LastName</li>
            }
        </ul>
    </feature>